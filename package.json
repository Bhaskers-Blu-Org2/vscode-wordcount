{
	"name": "wordcount",
	"version": "0.1.0",
	"publisher": "seanmcbreen",
	"description": "Markdown Word Count Example - a status bar contribution that updated for the number of workds in a Markdown document.",
	"author": {
		"name": "seanmcbreen"
	},
	"categories": [
		"Other"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "images/icon.svg",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:markdown"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-wordcount.git"
	},
	"engines": {
		"vscode": "0.10.x"
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "0.10.x"
	}
}